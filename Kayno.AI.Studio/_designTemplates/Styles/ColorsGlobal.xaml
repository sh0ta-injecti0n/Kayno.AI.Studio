<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	>


	<Color x:Key="Primary">#2f5bf9</Color>
	<SolidColorBrush x:Key="SolidPrimary" Color="{StaticResource Primary}" />
	<!--
    <Color x:Key="Primary">#2d59f9</Color>
    <Color x:Key="PrimaryLight">#3b65ff</Color>
    <Color x:Key="PrimaryDark">#1240e6</Color>
    <Color x:Key="PrimaryDarkText">#f9fdff</Color>
	-->


	<Color x:Key="Secondary">#f7bd34</Color>
	<SolidColorBrush x:Key="SolidSecondary" Color="{StaticResource Secondary}" />

	<!--
    <Color x:Key="Secondary">#ffcc12</Color>
    <Color x:Key="SecondaryLight">#ffcc80</Color>
    <Color x:Key="SecondaryDark">#c29693</Color>
    <Color x:Key="SecondaryDarkText">#f9fdff</Color>
    <Color x:Key="Tertiary">#ffcc12</Color>
    <Color x:Key="TertiaryLight">#ffcc80</Color>
    <Color x:Key="TertiaryDark">#c29693</Color>
	-->


	<LinearGradientBrush x:Key="GradientMainAccent" StartPoint="0,1" EndPoint="1,0">
		<GradientStop Offset="0.00" Color="#2f5bf9" />
		<GradientStop Offset="0.37" Color="#a740d4" />
		<GradientStop Offset="0.56" Color="#d62ea9" />
		<GradientStop Offset="0.71" Color="#ef6293" />
		<GradientStop Offset="0.83" Color="#f39595" />
		<GradientStop Offset="0.93" Color="#f6bd9c" />
		<GradientStop Offset="1.00" Color="#f4e3be" />
	</LinearGradientBrush>


	<RadialGradientBrush x:Key="GradientPrimaryMain" GradientOrigin="0.5, 0.05">
		<GradientStop Offset="0.090" Color="#2f5bf9" />
		<GradientStop Offset="0.915" Color="#5c32d9" />
		<GradientStop Offset="0.936" Color="#261d26" />
	</RadialGradientBrush>


	<LinearGradientBrush x:Key="GradientPrimaryMainBorder" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Offset="0.0" Color="#fff" />
		<GradientStop Offset="1.0" Color="#000" />
	</LinearGradientBrush>



	<LinearGradientBrush x:Key="GradientControl" StartPoint="0.3,-1.0" EndPoint="0.6,3.0">
		<GradientStop Offset="0.095" Color="#6d727a" />
		<GradientStop Offset="0.910" Color="#44474c" />
	</LinearGradientBrush>


	<LinearGradientBrush x:Key="GradientControlBorder" StartPoint="0.48,-1.0" EndPoint="0.53,1.2">
		<GradientStop Offset="0.095" Color="#99FFFFFF" />
		<GradientStop Offset="0.905" Color="#00FFFFFF" />
	</LinearGradientBrush>


	<LinearGradientBrush x:Key="GradientControlBorder2" StartPoint="0, 0.25" EndPoint="1, 2">
		<GradientStop Offset="0.095" Color="#00FFFFFF" />
		<GradientStop Offset="0.570" Color="#14FFFFFF" />
		<GradientStop Offset="0.905" Color="#4CFFFFFF" />
	</LinearGradientBrush>
	<!--  for Textbox border  -->


	<LinearGradientBrush x:Key="GradientBackgroundDark" StartPoint="0,1" EndPoint="0,0">
		<GradientStop Offset="0.91" Color="#293132" />
		<GradientStop Offset="0.82" Color="#272e30" />
		<GradientStop Offset="0.70" Color="#232a2e" />
		<GradientStop Offset="0.55" Color="#21262c" />
		<GradientStop Offset="0.36" Color="#1d1f25" />
		<GradientStop Offset="0.00" Color="#19191f" />
	</LinearGradientBrush>


	<LinearGradientBrush x:Key="GradientBackgroundDarkToTransp" StartPoint="0,1" EndPoint="0,0">
		<GradientStop Offset="0.095" Color="#19191f" />
		<GradientStop Offset="0.910" Color="#00000000" />
	</LinearGradientBrush>



	<Color x:Key="Base1">#fff</Color>
	<Color x:Key="Base2">#f2f2f2</Color>
	<Color x:Key="Base3">#cccccc</Color>
	<Color x:Key="Base4">#6d727a</Color>
	<Color x:Key="Base5">#595e66</Color>
	<Color x:Key="Base6">#4e5154</Color>
	<Color x:Key="Base7">#44474c</Color>
	<Color x:Key="Base8">#383a3d</Color>

	<SolidColorBrush x:Key="SolidBase1" Color="{StaticResource Base1}" />
	<SolidColorBrush x:Key="SolidBase2" Color="{StaticResource Base2}" />
	<SolidColorBrush x:Key="SolidBase3" Color="{StaticResource Base3}" />
	<SolidColorBrush x:Key="SolidBase4" Color="{StaticResource Base4}" />
	<SolidColorBrush x:Key="SolidBase5" Color="{StaticResource Base5}" />
	<SolidColorBrush x:Key="SolidBase6" Color="{StaticResource Base6}" />
	<SolidColorBrush x:Key="SolidBase7" Color="{StaticResource Base7}" />
	<SolidColorBrush x:Key="SolidBase8" Color="{StaticResource Base8}" />

	<Color x:Key="BaseDark1">#293132</Color>
	<Color x:Key="BaseDark2">#272e30</Color>
	<Color x:Key="BaseDark3">#232a2e</Color>
	<Color x:Key="BaseDark4">#21262c</Color>
	<Color x:Key="BaseDark5">#1d1f25</Color>
	<Color x:Key="BaseDark6">#19191f</Color>

	<SolidColorBrush x:Key="SolidBaseDark1" Color="{StaticResource BaseDark1}" />
	<SolidColorBrush x:Key="SolidBaseDark2" Color="{StaticResource BaseDark2}" />
	<SolidColorBrush x:Key="SolidBaseDark3" Color="{StaticResource BaseDark3}" />
	<SolidColorBrush x:Key="SolidBaseDark4" Color="{StaticResource BaseDark4}" />
	<SolidColorBrush x:Key="SolidBaseDark5" Color="{StaticResource BaseDark5}" />
	<SolidColorBrush x:Key="SolidBaseDark6" Color="{StaticResource BaseDark6}" />


	<!--  Depricated  -->
	<Color x:Key="BaseLightest">#F9FaFF</Color>
	<Color x:Key="BaseGray100">#F2F3F5</Color>
	<Color x:Key="BaseGray200">#D7DBE0</Color>
	<Color x:Key="BaseGray300">#D7DBE0</Color>
	<Color x:Key="BaseGray400">#A9AEB8</Color>
	<Color x:Key="BaseGray500">#A9AEB8</Color>
	<Color x:Key="BaseGray600">#697080</Color>
	<Color x:Key="BaseGray800">#323336</Color>
	<Color x:Key="BaseGray900">#222326</Color>
	<Color x:Key="BaseGray950">#0F1829</Color>
	<Color x:Key="BaseDarkest">#021019</Color>


</ResourceDictionary>